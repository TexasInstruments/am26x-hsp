    <protocolconfiguration>
      <hostinterface value="Simulink">
        <visible>true</visible>
      </hostinterface>
      <loggingbufferauto>
        <value>true</value>
        <visible>true</visible>
      </loggingbufferauto>
      <loggingbuffersize value="1000">
        <visible>true</visible>
      </loggingbuffersize>
      <loggingbuffernum value="3">
        <visible>false</visible>
      </loggingbuffernum>
      <maxcontigsamples value="10">
        <visible>false</visible>
      </maxcontigsamples>
      <widthoftargetdoubleinbytes>4</widthoftargetdoubleinbytes>
      <maxnumofodtswiththirdpartyhost>15</maxnumofodtswiththirdpartyhost>
      <maxnumofodtentrieswiththirdpartyhost>7</maxnumofodtentrieswiththirdpartyhost>
      <isbyteaddressgranularityemulation>false</isbyteaddressgranularityemulation>
