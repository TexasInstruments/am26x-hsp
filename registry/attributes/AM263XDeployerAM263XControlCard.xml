<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<productinfo version="3.0">
  <name>AM263X Deployer</name>
  <targetname>AM263X ControlCard</targetname>
  <targetinitializationcall>am263x_HardwareInit_Fcn()</targetinitializationcall>
  <pilinfo>
    <Name>PIL on Serial</Name>
